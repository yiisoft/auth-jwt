{
    "name": "yiisoft/auth-jwt",
    "type": "library",
    "description": "Yii auth JWT method",
    "keywords": [
        "auth",
        "jwt",
        "middleware",
        "psr-15"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/auth-jwt/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/auth-jwt"
    },
    "require": {
        "php": "^7.4|^8.0",
        "yiisoft/auth": "^1.0",
        "yiisoft/json": "^1.0",
        "web-token/jwt-checker": "~2.0",
        "web-token/jwt-key-mgmt": "~2.0",
        "web-token/jwt-signature": "~2.0",
        "web-token/jwt-signature-algorithm-hmac": "~2.0",
        "web-token/jwt-signature-algorithm-ecdsa": "~2.0",
        "web-token/jwt-signature-algorithm-rsa": "~2.0"
    },
    "require-dev": {
        "nyholm/psr7": "^1.3",
        "infection/infection": "^0.17.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.15"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Auth\\Jwt\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Auth\\Jwt\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "config-plugin": {
            "common": "config/common.php",
            "params": "config/params.php"
        }
    }
}
